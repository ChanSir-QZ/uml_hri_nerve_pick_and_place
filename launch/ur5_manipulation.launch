  atics_config:=$HOME/my_robot_calibration.yaml tool_device_name:=/tmp/ttyUR tool_voltage:=24 tool_parity:=0 tool_baud_rate:=115200 tool_stop_bits:=1 tool_rx_idle_chars:=1.5 tool_tx_idle_bits:=3.5 use_tool_communication:
