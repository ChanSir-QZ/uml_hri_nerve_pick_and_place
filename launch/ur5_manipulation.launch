<?xml version="1.0"?>
<launch>

  <include file="$(find uml_hri_nerve_pick_and_place)/launch/gpd_ur5.launch" />

  <include file="$(find uml_hri_nerve_armada_workstation)/universal_robot/ur_e_gazebo/launch/gazebo_universal_robot.launch" >
    <arg name="model" value="ur5"/>
    <arg name="sim_workstation" value="true"/>
    <arg name="gripper" value="robotiq_2f_85"/>
    <arg name="wrist_cam" value="d435_millibar"/>  <!-- d435_millibar -->
  </include>

  <rosparam file="$(find uml_hri_nerve_pick_and_place)/cfg/ur5e_workstation_sim_params.yaml" />

  <!--
  <node name="pick_and_place_gazebo" pkg="uml_hri_nerve_pick_and_place" type="pick_and_place_gazebo" output="screen"/>
  <node name="spawn_object" pkg="uml_hri_nerve_pick_and_place" type="spawn_object.py" output="screen"/>
  -->

  <node name="Robotiq2FGripperTcpNode" pkg="robotiq_2f_gripper_control" type="Robotiq2FGripperTcpNode.py" args="10.10.10.42"/>

  <node name="robotiq_2f_gripper_action_server_node" pkg="robotiq_2f_gripper_action_server" type="robotiq_2f_gripper_action_server_node">
    <remap from="/gripper/goal" to="/robotiq_2f_85_gripper_controller/gripper_cmd/goal"/>
    <remap from="/input" to="/Robotiq2FGripperRobotInput"/>
    <remap from="/output" to="/Robotiq2FGripperRobotOutput"/>
  </node>

  <!--
  <node name="robotiq_2f_gripper_action_server_node" pkg="robotiq_2f_gripper_action_server" type="robotiq_2f_gripper_action_server_node"/>
  -->

